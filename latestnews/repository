
import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.repository.PagingAndSortingRepository;
import org.springframework.stereotype.Repository;

import com.example.News.Portal.Entity.Table2;

@Repository
public interface newstable5repo extends JpaRepository<Table2, Long>, PagingAndSortingRepository<Table2, Long> {
	 List<Table2> findAll();
    List<Table2> findByTitleAndId(String Title, Long id);
    List<Table2> findByTitleOrId(String Title, Long id);
    List<Table2> findByTitleIn(List<String> title);
    List<Table2> findByTitleStartingWith(String title);
    List<Table2> findByTitleContains(String title);
}

